        opacity="0.9"/>

  <!-- Small highlight -->
  <circle cx="13" cy="14" r="1.5" fill="#ffffff" opacity="0.8"/>

  <!-- Letter J -->
  <text x="16" y="20" font-family="Arial, sans-serif" font-size="12" font-weight="bold"
        text-anchor="middle" fill="#004E64">J</text>
</svg>