<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <!-- Optimized for 32x32 favicon -->
  <defs>
    <linearGradient id="faviconBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#004E64;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00A5CF;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="faviconWater" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#9FFFCB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00A5CF;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background circle -->
  <circle cx="16" cy="16" r="14" fill="url(#faviconBg)"/>

  <!-- Simplified water drop -->
  <path d="M16 8 C12 8, 8 12, 8 16 C8 20, 12 24, 16 24 C20 24, 24 20, 24 16 C24 12, 20 8, 16 8 Z"
